{
  "name": "mf-template-angular",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ts-node --transpile-only --files ./server/index.ts",
    "start:local": "run-s lint:scss & run-s serve:single-spa",
    "start:dev": "run-s build:single-spa & ts-node --files ./server/index.ts",
    "build": "ng build",
    "build-staging": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "build:single-spa": "ng build mf-template-angular --configuration production",
    "serve:single-spa": "ng s --project mf-template-angular --disable-host-check --port 4210 --live-reload false",
    "lint:scss": "stylelint \"src/**/*.scss\" \"!src/styles/**/*.scss\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "~16.2.12",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^16.2.12",
    "@angular/material": "~16.2.12",
    "@angular/material-moment-adapter": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "hsts": "^2.2.0",
    "moment": "^2.30.1",
    "rxjs": "^7.8.1",
    "single-spa": ">=4.0.0",
    "single-spa-angular": "^8.1.1",
    "style-loader": "^3.3.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^16.0.1",
    "@angular-devkit/build-angular": "^16.2.10",
    "@angular/cli": "~16.2.10",
    "@angular/compiler-cli": "^16.2.12",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.13.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "protractor": "~7.0.0",
    "stylelint": "^16.3.1",
    "stylelint-scss": "^6.2.1",
    "ts-node": "^10.9.1",
    "tslint": "~6.1.0",
    "typescript": "~5.1.6"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 120
  }
}
